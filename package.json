{
    "name": "grepfeed",
    "version": "3.0.0",
    "description": "Filters out rss/atom feeds. Returns articles matching a pattern. The output can be another valid xml feed.",
    "keywords": [
        "grep",
        "rss",
        "xml"
    ],
    "author": "Alexander Gromnitsky <alexander.gromnitsky@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gromnitsky/grepfeed.git"
    },
    "type": "module",
    "scripts": {
        "start": "node server/index.js _out"
    },
    "dependencies": {
        "addressparser": "~1.0.1",
        "ent": "~2.2.0",
        "feedparser": "~2.2.9",
        "lodash.get": "~4.4.2",
        "lodash.words": "~4.2.0",
        "mime": "~4.0.1",
        "minimist": "~1.2.8",
        "parse5": "~7.1.2"
    },
    "devDependencies": {
        "@babel/cli": "~7.23.9",
        "@babel/core": "~7.24.0",
        "@babel/preset-react": "~7.23.3",
        "@rollup/plugin-commonjs": "~25.0.7",
        "@rollup/plugin-node-resolve": "~15.2.3",
        "eslint-plugin-react": "~7.34.0",
        "nprogress": "~0.2.0",
        "react": "~17.0.2",
        "react-dom": "~17.0.2",
        "rollup": "~4.13.0",
        "rollup-plugin-node-polyfills": "~0.2.1",
        "shellwords-ts": "~3.0.1"
    },
    "files": [
        "cli/",
        "lib/",
        "server/"
    ],
    "bin": {
        "grepfeed": "cli/grepfeed.js",
        "grepfeed-server": "server/index.js"
    }
}
