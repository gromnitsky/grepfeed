{
    "name": "grepfeed",
    "version": "1.3.3",
    "description": "Filters out rss/atom feeds. Returns articles matching a pattern. The output can be another valid xml feed.",
    "keywords": [
        "grep",
        "rss",
        "xml"
    ],
    "author": "Alexander Gromnitsky <alexander.gromnitsky@gmail.com>",
    "license": "MIT",
    "repository": "github:gromnitsky/grepfeed",
    "type": "module",
    "scripts": {
        "start": "node server _out/production/client"
    },
    "dependencies": {
        "addressparser": "~1.0.1",
        "ent": "~2.2.0",
        "feedparser": "~2.2.9",
        "lodash.words": "~4.2.0",
        "mime": "~2.2.0",
        "minimist": "~1.2.0",
        "parse5": "~4.0.0",
        "pump": "~3.0.0",
        "request": "~2.88.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "babel-plugin-git-log-1": "~0.0.2",
        "eslint-plugin-react": "^7.18.3",
        "lodash.get": "~4.4.2",
        "nprogress": "~0.2.0",
        "react": "~16.3.1",
        "react-dom": "~16.3.1",
        "shell-quote": "~1.6.1"
    },
    "files": [
        "cli/",
        "lib/",
        "!lib/dom.js",
        "server/"
    ],
    "bin": {
        "grepfeed": "cli/grepfeed",
        "grepfeed-server": "server/index.js"
    }
}
